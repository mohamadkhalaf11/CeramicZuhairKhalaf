<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LoginFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:backgroundTint="#FFFFFF">

        <EditText
            android:id="@+id/etPasswordLoginFragment"
            android:layout_width="333dp"
            android:layout_height="61dp"
            android:layout_marginStart="39dp"
            android:layout_marginTop="26dp"
            android:layout_marginEnd="39dp"
            android:background="@drawable/edittexbackground"
            android:ems="10"
            android:hint="    Password"
            android:inputType="text"
            app:layout_constraintBottom_toTopOf="@+id/tvForgetPasswordLoginFramgment"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/etEmailLoginFragment"
            tools:ignore="MissingConstraints" />

        <ImageView
            android:id="@+id/ivLogoLoginFragment"
            android:layout_width="105dp"
            android:layout_height="105dp"
            android:layout_marginStart="150dp"
            android:layout_marginTop="90dp"
            android:layout_marginEnd="156dp"
            android:layout_marginBottom="10dp"
            android:backgroundTint="#00FFFFFF"
            android:src="@drawable/img"
            app:layout_constraintBottom_toTopOf="@+id/etEmailLoginFragment"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <Button
            android:id="@+id/btnSigninLoginFragment"
            android:layout_width="333dp"
            android:layout_height="61dp"
            android:layout_marginStart="39dp"
            android:layout_marginTop="24dp"
            android:layout_marginEnd="39dp"
            android:layout_marginBottom="10dp"
            android:backgroundTint="#46505c"
            android:text="Sign in"
            app:cornerRadius="7dp"
            app:layout_constraintBottom_toTopOf="@+id/tvSingupLoginFragment"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tvForgetPasswordLoginFramgment"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/tvForgetPasswordLoginFramgment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="149dp"
            android:layout_marginTop="26dp"
            android:layout_marginEnd="149dp"
            android:text="Forget Password?"
            android:textColor="#5d6265"
            app:layout_constraintBottom_toTopOf="@+id/btnSigninLoginFragment"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/etPasswordLoginFragment"
            tools:ignore="MissingConstraints" />

        <ImageButton
            android:id="@+id/btnBackLoginFragment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="17dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="347dp"
            android:layout_marginBottom="239dp"
            android:backgroundTint="#FFFFFF"
            android:contentDescription="@string/app_name"
            android:src="@drawable/baseline_arrow_back_24"
            app:layout_constraintBottom_toTopOf="@+id/etEmailLoginFragment"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints,TouchTargetSizeCheck" />

        <EditText
            android:id="@+id/etEmailLoginFragment"
            android:layout_width="333dp"
            android:layout_height="61dp"
            android:layout_marginStart="40dp"
            android:layout_marginTop="103dp"
            android:layout_marginEnd="39dp"
            android:background="@drawable/edittexbackground"
            android:ems="10"
            android:hint="    Email"
            android:inputType="textEmailAddress"
            app:layout_constraintBottom_toTopOf="@+id/etPasswordLoginFragment"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ivLogoLoginFragment"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/tvSingupLoginFragment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="109dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="109dp"
            android:layout_marginBottom="106dp"
            android:text="Don't have an account? Sing Up"
            android:textColor="#5d6265"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/btnSigninLoginFragment"
            tools:ignore="MissingConstraints" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>